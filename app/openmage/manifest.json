	"manifestVersion": "v1",
	"slugs": ["openmage", "magento1"],
	"name": "OpenMage",
	"type": "app",
	"description": "OpenMage allows merchants already using an e-commerce site under Magento 1 to continue using a secure and stable Magento platform while remaining PCI compliant.",
	"services": ["php:8.2", "mariadb"],
